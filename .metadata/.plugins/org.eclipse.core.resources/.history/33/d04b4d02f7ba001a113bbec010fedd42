package JavaBasics;

public class ArrayConcepts {

	public static void main(String[] args) {
		int i;
		
		int a[i]=new int[20];
		 a[0]=2;
		 a[1]=3;
		 a[2]=4;
		 a[3]=5;
		 
		 System.out.println(a[1]);
		
		 System.out.println(a[2]);
		 System.out.println("Size of array is" (i.length()));
		
		// TODO Auto-generated method stub

	}

}
